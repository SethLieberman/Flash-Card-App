


	
		<%= form_for @profile, remote: true, html: {method: :patch}, html: {id: "edit-profile"} do |c| %>
			<%= c.label :First_Name %>
			<%= c.text_field :fname %><br>
			<%= c.label :Last_Name %>
			<%= c.text_field :lname %><br>
			<%= c.label :Username %>
			<%= c.text_field :username %><br>
			<%= c.submit :Edit %>
		<% end %>
	